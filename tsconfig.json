{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],

      "~/*": ["./src/components/*"],
      "#/*": ["./src/constants/*"],
      "assets/*": ["./src/assets/*"],
      "styles/*": ["./src/stylesheets/*"],
      "packages/*": ["./src/packages/*"],
      "instance/*": ["./src/instance/*"]
    }
  }
}
